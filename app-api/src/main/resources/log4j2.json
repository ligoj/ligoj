{
	"configuration": {
		"status": "warn",
		"name": "Main",
		"monitorInterval": 60,
		"properties": {
			"property": {
				"name": "filename",
				"value": "target/rolling.log"
			}
		},
		"ThresholdFilter": {
			"level": "debug"
		},
		"appenders": {
			"appender": [
				{
					"type": "Console",
					"name": "STDOUT",
					"PatternLayout": {
						"pattern": "%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %msg%xEx{filters(com.sun.proxy,org.springframework.data.projection,org.springframework.dao.support,org.springframework.transaction,java.lang.Thread,org.apache.cxf,org.eclipse.jetty,org.springframework.web,javax.servlet,org.springframework.aop,org.junit,org.apache.maven,sun.reflect,java.lang.reflect,org.springframework.cglib,org.springframework.security,org.springframework.boot.actuate,jdk.internal.reflect,org.springframework.cache.jcache.interceptor,java.util.stream,org.springframework.data.repository.core.support,org.springframework.data.jpa.repository.support,java.util.Iterator.forEachRemaining,java.util.Spliterators$IteratorSpliterator)}%n"
					}
				}, {
					"type": "Console",
					"name": "STDOUT_VERBOSE",
					"PatternLayout": {
						"pattern": "%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
					}
				}, {
					"type": "RollingFile",
					"name": "RollingFile",
					"fileName": "${filename}",
					"filePattern": "target/rolling-${sd:type}.%i.log.gz",
					"PatternLayout": {
						"pattern": "%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
					},
					"SizeBasedTriggeringPolicy": {
						"size": "500"
					}
				}
			]
		},
		"loggers": {
			"logger": [
				{
					"name": "org.springframework.security.web.FilterChainProxy",
					"level": "info",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.hibernate.SQL",
					"level": "${sys:log.sql:-info}",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.apache.cxf.phase.PhaseInterceptorChain",
					"level": "error",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.hibernate.ejb.HibernatePersistence",
					"level": "error",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.apache.directory.server.schema.registries.DefaultAttributeTypeRegistry",
					"level": "fatal",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.apache.directory.server.core.DefaultDirectoryService",
					"level": "error",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.apache.directory.server.protocol.shared.store.LdifFileLoader",
					"level": "warn",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.springframework.security.web.FilterChainProxy",
					"level": "info",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.springframework.ldap.core.LdapTemplate",
					"level": "warn",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.eclipse.jetty.server.Server",
					"level": "${sys:log.http:-info}",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.eclipse.jetty.webapp.StandardDescriptorProcessor",
					"level": "warn",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}				
			],
			"root": {
				"level": "info",
				"AppenderRef": {
					"ref": "STDOUT"
				}
			}
		}
	}
}