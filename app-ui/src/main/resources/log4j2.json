{
	"configuration": {
		"status": "warn",
		"name": "Main",
		"monitorInterval": 60,
		"properties": {
			"property": {
				"name": "filename",
				"value": "target/rolling.log"
			}
		},
		"ThresholdFilter": {
			"level": "debug"
		},
		"appenders": {
			"appender": [
				{
					"type": "Console",
					"name": "STDOUT",
					"PatternLayout": {
						"pattern": "%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %msg%n"
					}
				}, {
					"type": "Console",
					"name": "STDOUT_VERBOSE",
					"PatternLayout": {
						"pattern": "%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
					}
				}, {
					"type": "RollingFile",
					"name": "RollingFile",
					"fileName": "${filename}",
					"filePattern": "target/rolling-${sd:type}.%i.log.gz",
					"PatternLayout": {
						"pattern": "%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
					},
					"SizeBasedTriggeringPolicy": {
						"size": "500"
					}
				}
			]
		},
		"loggers": {
			"logger": [
				{
					"name": "org.springframework.security",
					"level": "warn",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.eclipse.jetty.server.Server",
					"level": "debug",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}, {
					"name": "org.eclipse.jetty.webapp.StandardDescriptorProcessor",
					"level": "warn",
					"additivity": "false",
					"AppenderRef": {
						"ref": "STDOUT"
					}
				}
			],
			"root": {
				"level": "info",
				"AppenderRef": {
					"ref": "STDOUT"
				}
			}
		}
	}
}