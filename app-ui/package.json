	"repository": {
		"type": "git",
		"url": "https://github.com/ligoj/ligoj"
	},
	"happiness": {
		"globals": ["jQuery", "DEBUG_ENABLED", "traceDebug", "traceLog", "window", "define", "_", "REST_PATH", "Handlebars", "momentManager", "applicationManager", "notifyManager", "validationManager", "$", "document", "navigator", "requirejs", "location", "bootbox", "errorManager", "formatManager", "moment", "securityManager"]
	},
	"eslintConfig": {
		"globals": {
			"jQuery" :true, "DEBUG_ENABLED" :true, "traceDebug" :true, "traceLog" :true, "window" :true, "define" :true, "_" :true, "REST_PATH" :true, "Handlebars" :true,
			"momentManager" :true, "applicationManager" :true, "notifyManager" :true, "validationManager" :true, "$" :true, "document" :true, "navigator" :true, 
			"requirejs" :true, "location" :true, "bootbox" :true, "errorManager" :true, "formatManager" :true, "moment" :true, "securityManager" :true
		}
	},
	"xo": {
		"globals": ["jQuery", "DEBUG_ENABLED", "traceDebug", "traceLog", "window", "define", "_", "REST_PATH", "Handlebars", "momentManager", "applicationManager", "notifyManager", "validationManager", "$", "document", "navigator", "requirejs", "location", "bootbox", "errorManager", "formatManager", "moment", "securityManager"],
		"rule": {
			"no-unneeded-ternary": 0,
			"dot-notation": 0,
			"import/no-amd": 0,
			"indent": 0,
			"max-lines": 0,
			"max-params": 0,
			"no-new": 0,
			"no-unused-expressions": 0,
			"import/no-dynamic-require": 0,
			"unicorn/explicit-length-check": 0,
			"quote-props": 0,
			"no-template-curly-in-string" :0
		},
		"envs": ["node", "window"]
	},
	"stylelint": {
		"rules": {
			"at-rule-no-unknown": [true, {
				"ignoreAtRules": [
					"include",
					"mixin"
				]
			}],
			"indentation": "tab",
			"selector-combinator-space-before": "never",
			"selector-combinator-space-after": "never",
			"selector-list-comma-newline-after": "never-multi-line",
			"rule-non-nested-empty-line-before": null,
			"media-feature-no-missing-punctuation": null,
			"number-leading-zero": "never"
		}
	},
	"private": true,
	"analyze": true,
	"devDependencies": {
		"node": ">=0.5.x",
		"happiness": ">7.x",
		"xo": "^0.12.0"
	},
